@startuml
'https://plantuml.com/class-diagram

+class programManager {
    - songToSinger: HashMap<String, Singer>
    - singer: HashMap<Integer, Singer>
    + selectAction(action: Action)
    - placeOrder(songNum: int, email: String, cardNum: int, sweetHeartName: String): boolean
    - generateReport(singerID: int)
    - singerCheckOut(singerID : int)
}

+class Order {
    - orderID: int
    - customerEmail: String
    - customerCardNum: int
    - sweetheartName: String
    - songOrdered: String
    + getSweetheartName(): String
    + chargeCard(): boolean
}

+class Singer {
    - id: int
    - name: String
    - songName: String
    - singOrders: ArrayList<Order>
    - isAvailable: boolean
    + generateEmailReport(): boolean
    + addOrder(order: Order): boolean
    + finishOrders(): boolean
}

+enum Action {
    ORDER,
    REPORT,
    DONE
}

programManager .u.> Action
programManager -->"*" Singer
Singer -->"*"Order
@enduml